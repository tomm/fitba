<div class="login">
	<h1>Fitba Login</h1>
<%= form_tag :action => 'try_login', :id => 'credentials' do %>
  <%= label_tag(:username, "Name:") %>
  <%= text_field_tag(:username) %>
  <br>
  <%= label_tag(:password, "Password:") %>
  <%= password_field_tag(:password) %>
  <br>
  <%= submit_tag("Log In") %>
<% end %>
</div>
